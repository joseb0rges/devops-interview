{
    "family": "task-api",
    "containerDefinitions": [
        {
            "cpu": 0,
            "environment": [],
            "essential": true,
            "image": "573412182393.dkr.ecr.us-east-1.amazonaws.com/api-webhook:latest",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/task-api",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "memoryReservation": 128,
            "mountPoints": [],
            "name": "api-webhook",
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000,
                    "protocol": "tcp"
                }
            ],
            "systemControls": [],
            "ulimits": [
                {
                    "hardLimit": 1000000,
                    "name": "nofile",
                    "softLimit": 100000
                }
            ],
            "volumesFrom": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::573412182393:role/api-ecsTaskRole",
    "executionRoleArn": "arn:aws:iam::573412182393:role/api-ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}